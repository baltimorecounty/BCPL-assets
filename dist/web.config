<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<system.webServer>
		<rewrite>
			<rules>
				<rule name="SEBIN" stopProcessing="true">
                    <match url="^sebin.+" />
                    <action type="Redirect" url="http://staging.bcpl.info/{R:0}" />
                </rule>
			</rules>
		</rewrite>
	</system.webServer>
</configuration>