<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<system.webServer>
		<rewrite>
			<rules>
				<rule name="fonts" stopProcessing="true">
					<match url="sebin\/\w\/\w\/(\w+\.(eot|woff|ttf|woff2))$" />
					<action type="Redirect" url="http://oit226696:3000/fonts/{R:1}" />
				</rule>
				<rule name="css" stopProcessing="true">
					<match url="sebin/\w/\w/([\w\-]+\.?[\w\-]+\.css)$" />
					<action type="Redirect" url="http://oit226696:3000/css/{R:1}" />
				</rule>
				<rule name="js" stopProcessing="true">
					<match url="sebin/\w/\w/([\w\-]+\.?[\w\-]+\.js)$" />
					<action type="Redirect" url="http://oit226696:3000/js/{R:1}" />
				</rule>
				<rule name="png" stopProcessing="true">
					<match url="sebin/\w/\w/([\w\-]+\.png)$" />
					<action type="Redirect" url="http://oit226696:3000/images/{R:1}" />
				</rule>
				<rule name="jpg" stopProcessing="true">
					<match url="sebin/\w/\w/([\w\-]+\.jpg)$" />
					<action type="Redirect" url="http://oit226696:3000/images/{R:1}" />
				</rule>
				<rule name="json" stopProcessing="true">
					<match url="sebin/\w/\w/([\w\-]+\.json)$" />
					<action type="Redirect" url="http://oit226696:3000/data/{R:1}" />
				</rule>
				<rule name="local-js">
					<match url="_js/(.+)" />
					<action type="Rewrite" url="js/{R:1}" />
				</rule>
			</rules>
		</rewrite>
	</system.webServer>
</configuration>